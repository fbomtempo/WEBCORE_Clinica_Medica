@model IEnumerable<WEBCORE_Clinica_Medica.Models.Dominio.Medico>

@{
    ViewData["Title"] = "Gerar Médicos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1 class="pb-3">Gerar Médicos</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.sobrenome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.nascimento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.especialidade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.crm)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.nome)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sobrenome)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.nascimento)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.especialidade)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.crm)
                </td>
            </tr>
        }
    </tbody>
</table>
